package ssapackage;

public class TestCustomers {

	public static void main(String[] args) {
		Customer c1, c2;
		c1 = new Customer();
		c2 =new Customer();
		
		System.out.println(c1.custName);
		System.out.println(c1.custID);
		
		c1.custName = "Mary";
		c1.custID = 11;
		
		System.out.println(c1.custName);
		System.out.println(c1.custID);

	}//main
}//class
